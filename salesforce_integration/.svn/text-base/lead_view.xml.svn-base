<!--	    Contracts form inherited-->


        <record id="sf_account_analytic_account_form_inherited" model="ir.ui.view">
		    <field name="name">Contracts</field>
		    <field name="model">account.analytic.account</field>
		    <field name="type">form</field>
		    <field name="inherit_id" ref="account_analytic_analysis.account_analytic_account_form_form"/>
		    <field name="arch" type="xml">
                 <page string="Contract Information" position="after">
			        <page string="Salesforce" >
			            <group>
			                <group>
                                <button name="delete_contract_from_salesforce" string="Delete From SalesForce" type="object"/> 
<!--       Must added attrs="{'invisible': [('ContractNumber','=',False)]}" once Contract Number is imported-->
                            </group>
			                <group>
                            </group>
			                <group>                     
                                <field name='sf_customer_id'/>   
                                <field name='CustomerSignedTitle'/>             
                                <field name='CustomerSignedDate'/>
                                <field name='OwnerExpirationNotice'/>
                                <field name='CompanySignedDate'/>                                
                                <field name='BillingStreet' /> 
                                <field name='ContractNumber'/> 
                            </group>  
                            <group>                                 
                                <field name='BillingCity'/>                                                                  
                                <field name='BillingPostalCode'/> 
                                <field name='state_id'/> 
                                <field name='country_id'/>
                                <field name='sf_status'/>
                                <field name='active'/>
                                <field name='LastActivityDate'/>
                            </group>
                        </group>
                        <label for='SpecialTerms'/>
                        <field name='SpecialTerms'/>     
                    </page>
                </page>
	        </field>
	    </record>
	    
<!--	    Claims form inheritted-->


	    <record id="crm_claim_form_inherit" model="ir.ui.view">
		    <field name="name">sf.crm.claim.form.inherit</field>
		    <field name="model">crm.claim</field>
		    <field name="type">form</field>
		    <field name="inherit_id" ref="crm_claim.crm_case_claims_form_view"/>
		    <field name="arch" type="xml">
		        <header position="inside">
		            <button name="delete_claim_from_salesforce" string="Delete From SalesForce" type="object" attrs="{'invisible': [('CaseNumber','=',False)]}"/>
		        </header>
			    <page string="Follow Up" position="after">
			        <page string="Sale Force claim" >
			            <group>
			                <group>	
				                <field name="Type"/>
				                <field name="Reason"/>
				                <field name="SuppliedCompany"/>
				                <field name="SuppliedName"/>
				                <field name="ParentId"/>
				                <field name="Product_code"/>
				                <field name="sf_contact_id"/>
				                <field name="LastModifiedDate"/>
			                </group>
			                <group>                
				                <field name="CaseNumber" readonly="1"/>
				                <field name="Origin"/>
<!-- 				                <field name="PotentialLiability__c"/>
				                <field name="SLAViolation__c"/>
				                <field name="EngineeringReqNumber__c"/>
 -->
				                <field name="AssetId"/>
 				                <field name="IsEscalated"/>
				                <field name="IsClosed"/>
				            </group>
			            </group>
                    </page>
                </page>
	        </field>
	    </record>
    </data>
</openerp>


