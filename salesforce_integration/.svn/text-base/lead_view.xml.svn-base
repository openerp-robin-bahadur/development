<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
    
<!--        inherit crm.lead form view-->


	    <record id="crm_lead_form_inherit" model="ir.ui.view">
		    <field name="name">sf.crm.lead.form.inherit</field>
		    <field name="model">crm.lead</field>
		    <field name="type">form</field>
		    <field name="inherit_id" ref="crm.crm_case_form_view_leads"/>
		    <field name="arch" type="xml">
			    <page string="Extra Info" position="after">
			        <page string="Sale Force" >
			            <group>
			                <group>
                                <button name="delete_lead_from_salesforce" string="Delete From SalesForce" type="object" attrs="{'invisible': [('Status','=',False)]}"/> 
                            </group>
			                <group>
                            </group>
			                <group>	
			                    <field name="LeadSource"/>
				                <field name="Industry"/>
				                <field name="Status"/>			                				                
                                <field name="AnnualRevenue"/>
				                <field name="Title"/>				                
				                <field name='sf_country' readonly='1'/>
				                <field name='sf_state' readonly='1'/>
				                <field name="NumberOfEmployees"/>
				                <field name='CurrentGenerators__c'/>	
			                </group>
			                <group>	
<!-- 			                    <field name='SICCode__c'/>
			                    <field name='NumberofLocations__c'/>
			                    <field name='Primary__c'/>		               				                				                
 -->
				                <field name="LastActivityDate" readonly='1'/>
				                <field name="LastReferencedDate" readonly='1'/>
				                <field name="ConvertedDate" readonly='1'/> 				                
				                <field name="ConvertedOpportunityId" readonly='1'/>
				                <field name="ConvertedContactId" readonly='1'/>
				                <field name="ConvertedAccountId" readonly='1'/>
				            </group>
			            </group>
                    </page>
                </page>
                <field name='contact_name' position="after">
                        <field name='contact_name_last' placeholder="Last Name"/>                    
                </field>
                <field name='country_id' position="after">
                        <field name='website' />                    
                </field>
                
	        </field>
	    </record>
	    
<!--	    inherit opportunity form-->

	    <record id="sf_crm_oppertunity_form_inherit" model="ir.ui.view">
		    <field name="name">Opportunities</field>
		    <field name="model">crm.lead</field>
		    <field name="type">form</field>
		    <field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
		    <field name="arch" type="xml">
			    <page string="Lead" position="after">
			        <page string="SalesForce" >
			            <group>
			                <group>
                                <button name="delete_opportunity_from_salesforce" string="Delete From SalesForce" type="object" /> 
<!--attrs="{'invisible': [('StageName','=',False)]}"-->
                            </group>
			                <group>
                            </group>
			                <group>
			                    <field name="StageName"/>
				                <field name="Type"/>
				                <field name="ForecastCategoryName"/>
				                <field name='sf_country'/>
				                <field name='sf_state'/>				                
                            </group>
                            <group>
                                <field name="MainCompetitors__c"/>
				                <field name="DeliveryInstallationStatus__c"/>			                				                
                                <field name="CurrentGenerators__c"/>
				                <field name="TrackingNumber__c"/>
				                <field name="OrderNumber__c"/>
				            </group>
			            </group>
                    </page>
                </page> 
                </field>               
	    </record>
	    
<!--	    res.partner form inherited-->


        <record id="sf_partner_view_partner_form_inherited" model="ir.ui.view">
